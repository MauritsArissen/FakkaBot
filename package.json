{
  "name": "fakkabot",
  "version": "1.0.0",
  "description": "",
  "main": "src/bot.ts",
  "scripts": {
    "start": "nodemon --legacy-watch \"src/**\" --ext \"js,ts,json\" --exec \"ts-node --files src/bot.ts\"",
    "build": "tsc -p .",
    "eslint:github-action": "npx prisma generate --schema=./src/database/prd.prisma & eslint ./src",
    "pushDatabase:dev": "cd src/database & concat -o runtime.prisma ./DEV.prisma ./schema.prisma & cd ../.. & npx prisma db push --schema=./src/database/runtime.prisma",
    "pushDatabase:prd": "cd src/database & concat -o runtime.prisma ./PRD.prisma ./schema.prisma & cd ../.. & npx prisma db push --schema=./src/database/runtime.prisma --accept-data-loss"
  },
  "author": "Maurits Arissen",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^1.5.0",
    "@prisma/client": "^4.11.0",
    "axios": "^1.1.3",
    "canvacord": "^5.4.8",
    "canvas": "^2.8.0",
    "colorette": "^2.0.19",
    "concat": "^1.0.3",
    "discord.js": "^14.8.0",
    "dotenv": "^16.0.3",
    "moment": "^2.29.4",
    "node-fetch": "^3.2.10",
    "path": "^0.12.7",
    "pg": "^8.7.3",
    "prisma": "^4.11.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.11.0",
    "sequelize-typescript": "^2.1.1",
    "sqlite3": "^5.0.2",
    "tsyringe": "^4.7.0",
    "yuri-canvas": "^4.0.6"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-plugin-sonarjs": "^0.15.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
