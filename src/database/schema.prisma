datasource db {
  provider = "sqlite"
  url      = "file:../../database.sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model Logs {
  id        Int      @id @default(autoincrement())
  type      String?
  message   String?
  payload   String?
  timestamp DateTime
}

model UserStats {
  uid String @id
  xp  Int
}
